{
  "version": 2,
  "builds": [
    {
      "src": "spotter_api/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "staticfiles_build/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "spotter_api/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "spotter_api.settings"
  }
}
